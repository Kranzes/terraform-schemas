{
  "format_version": "1.0",
  "provider_schemas": {
    "registry.terraform.io/pan-net/powerdns": {
      "provider": {
        "version": 0,
        "block": {
          "attributes": {
            "api_key": {
              "type": "string",
              "description": "REST API authentication key",
              "description_kind": "plain",
              "required": true
            },
            "ca_certificate": {
              "type": "string",
              "description": "Content or path of a Root CA to be used to verify PowerDNS's SSL certificate",
              "description_kind": "plain",
              "optional": true
            },
            "cache_mem_size": {
              "type": "string",
              "description": "Set cache memory size in MB",
              "description_kind": "plain",
              "optional": true
            },
            "cache_requests": {
              "type": "bool",
              "description": "Enable cache REST API requests",
              "description_kind": "plain",
              "optional": true
            },
            "cache_ttl": {
              "type": "number",
              "description": "Set cache TTL in seconds",
              "description_kind": "plain",
              "optional": true
            },
            "insecure_https": {
              "type": "bool",
              "description": "Disable verification of the PowerDNS server's TLS certificate",
              "description_kind": "plain",
              "optional": true
            },
            "server_url": {
              "type": "string",
              "description": "Location of PowerDNS server",
              "description_kind": "plain",
              "required": true
            }
          },
          "description_kind": "plain"
        }
      },
      "resource_schemas": {
        "powerdns_record": {
          "version": 0,
          "block": {
            "attributes": {
              "id": {
                "type": "string",
                "description_kind": "plain",
                "optional": true,
                "computed": true
              },
              "name": {
                "type": "string",
                "description_kind": "plain",
                "required": true
              },
              "records": {
                "type": [
                  "set",
                  "string"
                ],
                "description_kind": "plain",
                "required": true
              },
              "set_ptr": {
                "type": "bool",
                "description": "For A and AAAA records, if true, create corresponding PTR.",
                "description_kind": "plain",
                "optional": true
              },
              "ttl": {
                "type": "number",
                "description_kind": "plain",
                "required": true
              },
              "type": {
                "type": "string",
                "description_kind": "plain",
                "required": true
              },
              "zone": {
                "type": "string",
                "description_kind": "plain",
                "required": true
              }
            },
            "description_kind": "plain"
          }
        },
        "powerdns_zone": {
          "version": 0,
          "block": {
            "attributes": {
              "account": {
                "type": "string",
                "description_kind": "plain",
                "optional": true
              },
              "id": {
                "type": "string",
                "description_kind": "plain",
                "optional": true,
                "computed": true
              },
              "kind": {
                "type": "string",
                "description_kind": "plain",
                "required": true
              },
              "masters": {
                "type": [
                  "set",
                  "string"
                ],
                "description_kind": "plain",
                "optional": true
              },
              "name": {
                "type": "string",
                "description_kind": "plain",
                "required": true
              },
              "nameservers": {
                "type": [
                  "set",
                  "string"
                ],
                "description_kind": "plain",
                "optional": true
              },
              "soa_edit_api": {
                "type": "string",
                "description_kind": "plain",
                "optional": true
              }
            },
            "description_kind": "plain"
          }
        }
      }
    }
  }
}
