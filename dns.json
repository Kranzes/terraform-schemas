{
  "format_version": "1.0",
  "provider_schemas": {
    "registry.terraform.io/hashicorp/dns": {
      "provider": {
        "version": 0,
        "block": {
          "block_types": {
            "update": {
              "nesting_mode": "list",
              "block": {
                "attributes": {
                  "key_algorithm": {
                    "type": "string",
                    "description": "Required if `key_name` is set. When using TSIG authentication, the algorithm to use for HMAC. Valid values are `hmac-md5`, `hmac-sha1`, `hmac-sha256` or `hmac-sha512`. Value can also be sourced from the DNS_UPDATE_KEYALGORITHM environment variable.",
                    "description_kind": "plain",
                    "optional": true
                  },
                  "key_name": {
                    "type": "string",
                    "description": "The name of the TSIG key used to sign the DNS update messages. Value can also be sourced from the DNS_UPDATE_KEYNAME environment variable.",
                    "description_kind": "plain",
                    "optional": true
                  },
                  "key_secret": {
                    "type": "string",
                    "description": "Required if `key_name` is set\nA Base64-encoded string containing the shared secret to be used for TSIG. Value can also be sourced from the DNS_UPDATE_KEYSECRET environment variable.",
                    "description_kind": "plain",
                    "optional": true
                  },
                  "port": {
                    "type": "number",
                    "description": "The target UDP port on the server where updates are sent to. Defaults to `53`. Value can also be sourced from the DNS_UPDATE_PORT environment variable.",
                    "description_kind": "plain",
                    "optional": true
                  },
                  "retries": {
                    "type": "number",
                    "description": "How many times to retry on connection timeout. Defaults to `3`. Value can also be sourced from the DNS_UPDATE_RETRIES environment variable.",
                    "description_kind": "plain",
                    "optional": true
                  },
                  "server": {
                    "type": "string",
                    "description": "The hostname or IP address of the DNS server to send updates to. Value can also be sourced from the DNS_UPDATE_SERVER environment variable.",
                    "description_kind": "plain",
                    "optional": true
                  },
                  "timeout": {
                    "type": "string",
                    "description": "Timeout for DNS queries. Valid values are durations expressed as `500ms`, etc. or a plain number which is treated as whole seconds. Value can also be sourced from the DNS_UPDATE_TIMEOUT environment variable.",
                    "description_kind": "plain",
                    "optional": true
                  },
                  "transport": {
                    "type": "string",
                    "description": "Transport to use for DNS queries. Valid values are `udp`, `udp4`, `udp6`, `tcp`, `tcp4`, or `tcp6`. Any UDP transport will retry automatically with the equivalent TCP transport in the event of a truncated response. Defaults to `udp`. Value can also be sourced from the DNS_UPDATE_TRANSPORT environment variable.",
                    "description_kind": "plain",
                    "optional": true
                  }
                },
                "block_types": {
                  "gssapi": {
                    "nesting_mode": "list",
                    "block": {
                      "attributes": {
                        "keytab": {
                          "type": "string",
                          "description": "This or `password` is required if `username` is set, not supported on Windows. The path to a keytab file containing a key for `username`. Value can also be sourced from the DNS_UPDATE_KEYTAB environment variable.",
                          "description_kind": "plain",
                          "optional": true
                        },
                        "password": {
                          "type": "string",
                          "description": "This or `keytab` is required if `username` is set. The matching password for `username`. Value can also be sourced from the DNS_UPDATE_PASSWORD environment variable.",
                          "description_kind": "plain",
                          "optional": true,
                          "sensitive": true
                        },
                        "realm": {
                          "type": "string",
                          "description": "The Kerberos realm or Active Directory domain. Value can also be sourced from the DNS_UPDATE_REALM environment variable.",
                          "description_kind": "plain",
                          "optional": true
                        },
                        "username": {
                          "type": "string",
                          "description": "The name of the user to authenticate as. If not set the current user session will be used. Value can also be sourced from the DNS_UPDATE_USERNAME environment variable.",
                          "description_kind": "plain",
                          "optional": true
                        }
                      },
                      "description": "A `gssapi` block. Only one `gssapi` block may be in the configuration. Conflicts with use of `key_name`, `key_algorithm` and `key_secret`.",
                      "description_kind": "plain"
                    }
                  }
                },
                "description": "When the provider is used for DNS updates, this block is required. Only one `update` block may be in the configuration.",
                "description_kind": "plain"
              }
            }
          },
          "description_kind": "plain"
        }
      },
      "resource_schemas": {
        "dns_a_record_set": {
          "version": 0,
          "block": {
            "attributes": {
              "addresses": {
                "type": [
                  "set",
                  "string"
                ],
                "description": "The IPv4 addresses this record set will point to.",
                "description_kind": "plain",
                "required": true
              },
              "id": {
                "type": "string",
                "description_kind": "plain",
                "optional": true,
                "computed": true
              },
              "name": {
                "type": "string",
                "description": "The name of the record set. The `zone` argument will be appended to this value to create the full record path.",
                "description_kind": "plain",
                "optional": true
              },
              "ttl": {
                "type": "number",
                "description": "The TTL of the record set. Defaults to `3600`.",
                "description_kind": "plain",
                "optional": true
              },
              "zone": {
                "type": "string",
                "description": "DNS zone the record set belongs to. It must be an FQDN, that is, include the trailing dot.",
                "description_kind": "plain",
                "required": true
              }
            },
            "description": "Creates an A type DNS record set.",
            "description_kind": "plain"
          }
        },
