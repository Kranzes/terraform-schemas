{
  "format_version": "1.0",
  "provider_schemas": {
    "registry.terraform.io/gavinbunney/kubectl": {
      "provider": {
        "version": 0,
        "block": {
          "attributes": {
            "apply_retry_count": {
              "type": "number",
              "description": "Defines the number of attempts any create/update action will take",
              "description_kind": "plain",
              "optional": true
            },
            "client_certificate": {
              "type": "string",
              "description": "PEM-encoded client certificate for TLS authentication.",
              "description_kind": "plain",
              "optional": true
            },
            "client_key": {
              "type": "string",
              "description": "PEM-encoded client certificate key for TLS authentication.",
              "description_kind": "plain",
              "optional": true
            },
            "cluster_ca_certificate": {
              "type": "string",
              "description": "PEM-encoded root certificates bundle for TLS authentication.",
              "description_kind": "plain",
              "optional": true
            },
            "config_context": {
              "type": "string",
              "description_kind": "plain",
              "optional": true
            },
            "config_context_auth_info": {
              "type": "string",
              "description_kind": "plain",
              "optional": true
            },
            "config_context_cluster": {
              "type": "string",
              "description_kind": "plain",
              "optional": true
            },
            "config_path": {
              "type": "string",
              "description": "Path to the kube config file, defaults to ~/.kube/config",
              "description_kind": "plain",
              "optional": true
            },
            "config_paths": {
              "type": [
                "list",
                "string"
              ],
              "description": "A list of paths to kube config files. Can be set with KUBE_CONFIG_PATHS environment variable.",
              "description_kind": "plain",
              "optional": true
            },
            "host": {
              "type": "string",
              "description": "The hostname (in form of URI) of Kubernetes master.",
              "description_kind": "plain",
              "optional": true
            },
            "insecure": {
              "type": "bool",
              "description": "Whether server should be accessed without verifying the TLS certificate.",
              "description_kind": "plain",
              "optional": true
            },
            "load_config_file": {
              "type": "bool",
              "description": "Load local kubeconfig.",
              "description_kind": "plain",
              "optional": true
            },
            "password": {
              "type": "string",
              "description": "The password to use for HTTP basic authentication when accessing the Kubernetes master endpoint.",
              "description_kind": "plain",
              "optional": true
            },
            "token": {
              "type": "string",
              "description": "Token to authentifcate an service account",
              "description_kind": "plain",
              "optional": true
            },
            "username": {
              "type": "string",
              "description": "The username to use for HTTP basic authentication when accessing the Kubernetes master endpoint.",
              "description_kind": "plain",
              "optional": true
            }
          },
          "block_types": {
            "exec": {
              "nesting_mode": "list",
              "block": {
                "attributes": {
                  "api_version": {
                    "type": "string",
                    "description_kind": "plain",
                    "required": true
                  },
                  "args": {
                    "type": [
                      "list",
                      "string"
                    ],
                    "description_kind": "plain",
                    "optional": true
                  },
                  "command": {
                    "type": "string",
                    "description_kind": "plain",
                    "required": true
                  },
                  "env": {
                    "type": [
                      "map",
                      "string"
                    ],
                    "description_kind": "plain",
                    "optional": true
                  }
                },
                "description_kind": "plain"
              },
              "max_items": 1
            }
          },
          "description_kind": "plain"
        }
      },
      "resource_schemas": {
        "kubectl_manifest": {
          "version": 1,
          "block": {
            "attributes": {
              "api_version": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "apply_only": {
                "type": "bool",
                "description": "Apply only. In other words, it does not delete resource in any case.",
                "description_kind": "plain",
                "optional": true
              },
              "force_conflicts": {
                "type": "bool",
                "description": "Default false.",
                "description_kind": "plain",
                "optional": true
              },
              "force_new": {
                "type": "bool",
                "description": "Default to update in-place. Setting to true will delete and create the kubernetes instead.",
                "description_kind": "plain",
                "optional": true
              },
              "id": {
                "type": "string",
                "description_kind": "plain",
                "optional": true,
                "computed": true
              },
              "ignore_fields": {
                "type": [
                  "list",
                  "string"
                ],
                "description": "List of yaml keys to ignore changes to. Set these for fields set by Operators or other processes in kubernetes and as such you don't want to update.",
                "description_kind": "plain",
                "optional": true
              },
              "kind": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "live_manifest_incluster": {
                "type": "string",
                "description_kind": "plain",
                "computed": true,
                "sensitive": true
              },
              "live_uid": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "name": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "namespace": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "override_namespace": {
                "type": "string",
                "description": "Override the namespace to apply the kubernetes resource to",
                "description_kind": "plain",
                "optional": true
              },
              "sensitive_fields": {
                "type": [
                  "list",
                  "string"
                ],
                "description": "List of yaml keys with sensitive values. Set these for fields which you want obfuscated in the yaml_body output",
                "description_kind": "plain",
                "optional": true
              },
              "server_side_apply": {
                "type": "bool",
                "description": "Default to client-side-apply. Setting to true will use server-side apply.",
                "description_kind": "plain",
                "optional": true
              },
              "uid": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "validate_schema": {
                "type": "bool",
                "description": "Default to true (validate). Set this flag to not validate the yaml schema before appying.",
                "description_kind": "plain",
                "optional": true
              },
              "wait": {
                "type": "bool",
                "description": "Default to false (not waiting). Set this flag to wait or not for any deleted resources to be gone. This waits for finalizers.",
                "description_kind": "plain",
                "optional": true
              },
              "wait_for_rollout": {
                "type": "bool",
                "description": "Default to true (waiting). Set this flag to wait or not for Deployments and APIService to complete rollout",
                "description_kind": "plain",
                "optional": true
              },
              "yaml_body": {
                "type": "string",
                "description_kind": "plain",
                "required": true,
                "sensitive": true
              },
              "yaml_body_parsed": {
                "type": "string",
                "description": "Yaml body that is being applied, with sensitive values obfuscated",
                "description_kind": "plain",
                "computed": true
              },
              "yaml_incluster": {
                "type": "string",
                "description_kind": "plain",
                "computed": true,
                "sensitive": true
              }
            },
            "block_types": {
              "timeouts": {
                "nesting_mode": "single",
                "block": {
                  "attributes": {
                    "create": {
                      "type": "string",
                      "description_kind": "plain",
                      "optional": true
                    }
                  },
                  "description_kind": "plain"
                }
              }
            },
            "description_kind": "plain"
          }
        },
        "kubectl_server_version": {
          "version": 0,
          "block": {
            "attributes": {
              "build_date": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "git_commit": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "git_version": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "id": {
                "type": "string",
                "description_kind": "plain",
                "optional": true,
                "computed": true
              },
              "major": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "minor": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "patch": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "platform": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "triggers": {
                "type": [
                  "map",
                  "string"
                ],
                "description_kind": "plain",
                "optional": true
              },
              "version": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              }
            },
            "description_kind": "plain"
          }
        }
      },
      "data_source_schemas": {
        "kubectl_file_documents": {
          "version": 0,
          "block": {
            "attributes": {
              "content": {
                "type": "string",
                "description_kind": "plain",
                "required": true
              },
              "documents": {
                "type": [
                  "list",
                  "string"
                ],
                "description_kind": "plain",
                "computed": true
              },
              "id": {
                "type": "string",
                "description_kind": "plain",
                "optional": true,
                "computed": true
              },
              "manifests": {
                "type": [
                  "map",
                  "string"
                ],
                "description_kind": "plain",
                "computed": true
              }
            },
            "description_kind": "plain"
          }
        },
        "kubectl_filename_list": {
          "version": 0,
          "block": {
            "attributes": {
              "basenames": {
                "type": [
                  "list",
                  "string"
                ],
                "description_kind": "plain",
                "computed": true
              },
              "id": {
                "type": "string",
                "description_kind": "plain",
                "optional": true,
                "computed": true
              },
              "matches": {
                "type": [
                  "list",
                  "string"
                ],
                "description_kind": "plain",
                "computed": true
              },
              "pattern": {
                "type": "string",
                "description_kind": "plain",
                "required": true
              }
            },
            "description_kind": "plain"
          }
        },
        "kubectl_path_documents": {
          "version": 0,
          "block": {
            "attributes": {
              "disable_template": {
                "type": "bool",
                "description": "Flag to disable template parsing of the loaded documents",
                "description_kind": "plain",
                "optional": true
              },
              "documents": {
                "type": [
                  "list",
                  "string"
                ],
                "description_kind": "plain",
                "computed": true
              },
              "id": {
                "type": "string",
                "description_kind": "plain",
                "optional": true,
                "computed": true
              },
              "manifests": {
                "type": [
                  "map",
                  "string"
                ],
                "description_kind": "plain",
                "computed": true
              },
              "pattern": {
                "type": "string",
                "description_kind": "plain",
                "required": true
              },
              "sensitive_vars": {
                "type": [
                  "map",
                  "string"
                ],
                "description": "Sensitive variables to substitute, allowing for hiding sensitive variables in terraform output",
                "description_kind": "plain",
                "optional": true,
                "sensitive": true
              },
              "vars": {
                "type": [
                  "map",
                  "string"
                ],
                "description": "Variables to substitute",
                "description_kind": "plain",
                "optional": true
              }
            },
            "description_kind": "plain"
          }
        },
        "kubectl_server_version": {
          "version": 0,
          "block": {
            "attributes": {
              "build_date": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "git_commit": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "git_version": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "id": {
                "type": "string",
                "description_kind": "plain",
                "optional": true,
                "computed": true
              },
              "major": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "minor": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "patch": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "platform": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "version": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              }
            },
            "description_kind": "plain"
          }
        }
      }
    }
  }
}
