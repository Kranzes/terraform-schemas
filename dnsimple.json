{
  "format_version": "1.0",
  "provider_schemas": {
    "registry.terraform.io/dnsimple/dnsimple": {
      "provider": {
        "version": 0,
        "block": {
          "attributes": {
            "account": {
              "type": "string",
              "description": "The account for API operations.",
              "description_kind": "markdown",
              "optional": true
            },
            "prefetch": {
              "type": "bool",
              "description": "Flag to enable the prefetch of zone records.",
              "description_kind": "markdown",
              "optional": true
            },
            "sandbox": {
              "type": "bool",
              "description": "Flag to enable the sandbox API.",
              "description_kind": "markdown",
              "optional": true
            },
            "token": {
              "type": "string",
              "description": "The API v2 token for API operations.",
              "description_kind": "markdown",
              "optional": true,
              "sensitive": true
            },
            "user_agent": {
              "type": "string",
              "description": "Custom string to append to the user agent used for sending HTTP requests to the API.",
              "description_kind": "plain",
              "optional": true
            }
          },
          "description": "The DNSimple provider is used to interact with the various services that DNSimple offers. The provider needs to be configured with the proper credentials before it can be used.",
          "description_kind": "markdown"
        }
      },
      "resource_schemas": {
        "dnsimple_domain": {
          "version": 0,
          "block": {
            "attributes": {
              "account_id": {
                "type": "number",
                "description_kind": "plain",
                "computed": true
              },
              "auto_renew": {
                "type": "bool",
                "description_kind": "plain",
                "computed": true
              },
              "id": {
                "type": "number",
                "description_kind": "plain",
                "computed": true
              },
              "name": {
                "type": "string",
                "description_kind": "plain",
                "required": true
              },
              "private_whois": {
                "type": "bool",
                "description_kind": "plain",
                "computed": true
              },
              "registrant_id": {
                "type": "number",
                "description_kind": "plain",
                "computed": true
              },
              "state": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "unicode_name": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              }
            },
            "description": "DNSimple domain resource",
            "description_kind": "markdown"
          }
        },
        "dnsimple_email_forward": {
          "version": 0,
          "block": {
            "attributes": {
              "alias_email": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "alias_name": {
                "type": "string",
                "description_kind": "plain",
                "required": true
              },
              "destination_email": {
                "type": "string",
                "description_kind": "plain",
                "required": true
              },
              "domain": {
                "type": "string",
                "description_kind": "plain",
                "required": true
              },
              "id": {
                "type": "number",
                "description_kind": "plain",
                "computed": true
              }
            },
            "description": "DNSimple email forward resource",
            "description_kind": "markdown"
          }
        },
        "dnsimple_lets_encrypt_certificate": {
          "version": 0,
          "block": {
            "attributes": {
              "authority_identifier": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "auto_renew": {
                "type": "bool",
                "description_kind": "plain",
                "required": true
              },
              "created_at": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "csr": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "domain_id": {
                "type": "string",
                "description_kind": "plain",
                "required": true
              },
              "expires_at": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "id": {
                "type": "number",
                "description_kind": "plain",
                "computed": true
              },
              "name": {
                "type": "string",
                "description_kind": "plain",
                "required": true
              },
              "signature_algorithm": {
                "type": "string",
                "description_kind": "plain",
                "optional": true
              },
              "state": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "updated_at": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "years": {
                "type": "number",
                "description_kind": "plain",
                "computed": true
              }
            },
            "description": "DNSimple Let's Encrypt certificate resource",
            "description_kind": "markdown"
          }
        },
        "dnsimple_zone_record": {
          "version": 1,
          "block": {
            "attributes": {
              "id": {
                "type": "number",
                "description_kind": "plain",
                "computed": true
              },
              "name": {
                "type": "string",
                "description_kind": "plain",
                "required": true
              },
              "priority": {
                "type": "number",
                "description_kind": "plain",
                "optional": true,
                "computed": true
              },
              "qualified_name": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "ttl": {
                "type": "number",
                "description_kind": "plain",
                "optional": true,
                "computed": true
              },
              "type": {
                "type": "string",
                "description_kind": "plain",
                "required": true
              },
              "value": {
                "type": "string",
                "description_kind": "plain",
                "required": true
              },
              "zone_id": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "zone_name": {
                "type": "string",
                "description_kind": "plain",
                "required": true
              }
            },
            "description": "DNSimple domain resource",
            "description_kind": "markdown"
          }
        }
      },
      "data_source_schemas": {
        "dnsimple_certificate": {
          "version": 0,
          "block": {
            "attributes": {
              "certificate_chain": {
                "type": [
                  "list",
                  "string"
                ],
                "description": "Certificate chain",
                "description_kind": "markdown",
                "computed": true
              },
              "certificate_id": {
                "type": "number",
                "description": "Certificate ID",
                "description_kind": "markdown",
                "required": true
              },
              "domain": {
                "type": "string",
                "description": "Domain name",
                "description_kind": "markdown",
                "required": true
              },
              "id": {
                "type": "string",
                "description_kind": "plain",
                "computed": true
              },
              "private_key": {
                "type": "string",
                "description": "Private key",
                "description_kind": "markdown",
                "computed": true
              },
              "root_certificate": {
                "type": "string",
                "description": "Root certificate",
                "description_kind": "markdown",
                "computed": true
              },
              "server_certificate": {
                "type": "string",
                "description": "Server certificate",
                "description_kind": "markdown",
                "computed": true
              }
            },
            "description": "DNSimple certificate data source",
            "description_kind": "markdown"
          }
        },
        "dnsimple_zone": {
          "version": 0,
          "block": {
            "attributes": {
              "account_id": {
                "type": "number",
                "description": "DNSimple Account ID to which the zone belongs to",
                "description_kind": "markdown",
                "computed": true
              },
              "id": {
                "type": "number",
                "description_kind": "plain",
                "computed": true
              },
              "name": {
                "type": "string",
                "description": "Zone Name",
                "description_kind": "markdown",
                "required": true
              },
              "reverse": {
                "type": "bool",
                "description": "True if the zone is a reverse zone",
                "description_kind": "markdown",
                "computed": true
              }
            },
            "description": "DNSimple zone data source",
            "description_kind": "markdown"
          }
        }
      }
    }
  }
}
