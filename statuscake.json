                "description_kind": "plain",
                "computed": true
              }
            },
            "description_kind": "plain"
          }
        },
        "statuscake_pagespeed_monitoring_locations": {
          "version": 0,
          "block": {
            "attributes": {
              "id": {
                "type": "string",
                "description_kind": "plain",
                "optional": true,
                "computed": true
              },
              "locations": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "description": "string",
                      "ipv4": "string",
                      "ipv6": "string",
                      "region": "string",
                      "region_code": "string",
                      "status": "string"
                    }
                  ]
                ],
                "description": "List of monitoring locations",
                "description_kind": "plain",
                "computed": true
              },
              "region_code": {
                "type": "string",
                "description": "Location region code",
                "description_kind": "plain",
                "optional": true
              }
            },
            "description_kind": "plain"
          }
        },
        "statuscake_uptime_monitoring_locations": {
          "version": 0,
          "block": {
            "attributes": {
              "id": {
                "type": "string",
                "description_kind": "plain",
                "optional": true,
                "computed": true
              },
              "locations": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "description": "string",
                      "ipv4": "string",
                      "ipv6": "string",
                      "region": "string",
                      "region_code": "string",
                      "status": "string"
                    }
                  ]
                ],
                "description": "List of monitoring locations",
                "description_kind": "plain",
                "computed": true
              },
              "region_code": {
                "type": "string",
                "description": "Location region code",
                "description_kind": "plain",
                "optional": true
              }
            },
            "description_kind": "plain"
          }
        }
      }
    }
  }
}
