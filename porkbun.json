{
  "format_version": "1.0",
  "provider_schemas": {
    "registry.terraform.io/cullenmcdermott/porkbun": {
      "provider": {
        "version": 0,
        "block": {
          "attributes": {
            "api_key": {
              "type": "string",
              "description": "API Key for Porkbun",
              "description_kind": "markdown",
              "optional": true,
              "sensitive": true
            },
            "base_url": {
              "type": "string",
              "description": "Override Porkbun Base URL",
              "description_kind": "markdown",
              "optional": true
            },
            "max_retries": {
              "type": "number",
              "description": "Should only be changed if needing to work around Porkbun API rate limits",
              "description_kind": "markdown",
              "optional": true
            },
            "secret_key": {
              "type": "string",
              "description": "Secret Key for Porkbun",
              "description_kind": "markdown",
              "optional": true,
              "sensitive": true
            }
          },
          "description_kind": "plain"
        }
      },
      "resource_schemas": {
        "porkbun_dns_record": {
          "version": 0,
          "block": {
            "attributes": {
              "content": {
                "type": "string",
                "description": "The content of the record",
                "description_kind": "markdown",
                "optional": true
              },
              "domain": {
                "type": "string",
                "description": "The base domain to to create the record on",
                "description_kind": "markdown",
                "required": true
              },
              "id": {
                "type": "string",
                "description": "The Porkbun ID of the Record",
                "description_kind": "markdown",
                "computed": true
              },
              "name": {
                "type": "string",
                "description": "The subdomain for the record itself without the base domain",
                "description_kind": "markdown",
                "optional": true,
                "computed": true
              },
              "notes": {
                "type": "string",
                "description": "Notes to add to the record",
                "description_kind": "markdown",
                "optional": true
              },
              "prio": {
                "type": "string",
                "description": "The priority of the record",
                "description_kind": "markdown",
                "optional": true,
                "computed": true
              },
              "ttl": {
                "type": "string",
                "description": "The ttl of the record, the minimum  is 600",
                "description_kind": "markdown",
                "optional": true,
                "computed": true
              },
              "type": {
                "type": "string",
                "description": "The type of DNS Record to create",
                "description_kind": "markdown",
                "required": true
              }
            },
            "description": "Porkbun DNS Record resource",
            "description_kind": "markdown"
          }
        }
      }
    }
  }
}
