                  "username": {
                    "type": "string",
                    "description": "The username to use for HTTP basic authentication when accessing the Kubernetes master endpoint.",
                    "description_kind": "plain",
                    "required": true
                  }
                },
                "description": "RegistryClient configuration.",
                "description_kind": "plain"
              }
            }
